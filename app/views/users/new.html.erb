<h2>Sign Up</h2>
  <%= form_for @user do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :email %>
    <%= f.text_field :email %>

    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Submit" %>
  <% end %>

<%= link_to "Sign In if you already have an account", new_session_path %>
